<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}3D Design Studio{% endblock title %}</title>

        {% block stylesheets %}
            {% stylesheets
                "@ImperivSiteBundle/Resources/public/css/bootstrap.min.css"
                filter="yui_css"
            %}
                <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
            {% endstylesheets %}
        {% endblock stylesheets %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    </head>
    <body>
        <header>
            <ul class="navigator">
                <li>
                    <a href="{{ path('index', {'name':'Jack White'}) }}" {% if (app.request.get('_route') == 'index') %} class="selected" {% endif %}>Home</a>
                </li>
                <li>
                    <a href="#">3D Design</a>
                </li>
                <li>
                    <a href="#">Graphic Art</a>
                </li>
                <li>
                    <a href="#">Video Editing</a>
                </li>
                <li>
                    <a href="#">Models</a>
                </li>
                <li>
                    <a href="#" id="explore" >Explore more</a>
                </li>
            </ul>
        </header>
        {% block content %}{% endblock content %}
        {% block javascripts %}{% endblock javascripts %}
    </body>
</html>
